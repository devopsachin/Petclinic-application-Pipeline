@Library('pet-clinic')
import org.foo.*;

environment{ 
    def props = readJSON file: 'resources/pet-clinic.json'
    gitRepoUrl = "${props.repoUrl}"
    branch = "${props.gitBranch}"
 }

pipeline{
    agent any 
    stages{
    stage('Checkout SCM'){
                steps{
                       script{
                               pullGit(
                                    branch: "$branch",
                                    url: "$gitRepoUrl"
                        )
                        }
                    }
                }
    }
    }
